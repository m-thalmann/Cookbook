{
  "name": "m-thalmann/cookbook-api",
  "type": "project",
  "description": "The api for the cookbook client",
  "keywords": [
    "recipes",
    "cookbook",
    "rest",
    "api"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0.2",
    "ezyang/htmlpurifier": "^4.14",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^9.21",
    "laravel/tinker": "^2.7",
    "m-thalmann/laravel-token-auth": "^0.4.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "itsgoingd/clockwork": "^5.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "spatie/laravel-ignition": "^1.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit",
    "test-coverage": "./vendor/bin/phpunit --coverage-clover=coverage.xml --coverage-text",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
